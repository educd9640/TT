<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "ipn.escom.ballScore.entity.Escuela" table = "ESCUELA">
      
      <meta attribute = "class-description">
         This class contains the escuela detail. 
      </meta>
      
      <id name = "idEscuela" type = "java.lang.Long" column = "ID_ESCUELA">
         <generator class="sequence">
        	<param name="sequence">ESCUELA_SEQ</param>
    	 </generator>
      </id>
      
      <property name = "nombreEscuela" 			column = "NOMBRE_ESCUELA" 		type = "java.lang.String"	/>
      <property name = "nombreCortoEscuela" 	column = "NOMBRE_CORTO_ESCUELA" type = "java.lang.String"	/>
      <property name = "calle" 					column = "CALLE" 				type = "java.lang.String"	/>
      <property name = "colonia" 				column = "COLONIA" 				type = "java.lang.String"	/>
      <property name = "codigoPostal" 			column = "CODIGO_POSTAL" 		type = "java.lang.Long"		/>
      <property name = "delegacion" 			column = "DELEGACION" 			type = "java.lang.String"	/>
      <property name = "estado" 				column = "ESTADO" 				type = "java.lang.String"	/>
      <property name = "fechaAlta" 				column = "FECHA_ALTA" 			type = "java.sql.Date"		/>
      
   </class>
</hibernate-mapping>