<!DOCTYPE validators PUBLIC
         "-//Apache Struts//XWork Validator 1.0.3//EN"
           "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>
	<validator type="required">
		<param name="fieldname">partidoF.idTemporada</param>
		<message>Temporada requerida</message>
	</validator>
	<validator type="required">
		<param name="fieldname">partidoF.idEquipoLocal</param>
		<message>Equipo Local requerido</message>
	</validator>
	<validator type="required">
		<param name="fieldname">partidoF.idEquipoVisitante</param>
		<message>Equipo Visitante requerido</message>
	</validator>
	<validator type="required">
		<param name="fieldname">partidoF.campo</param>
		<message>Nombre del campo requerido</message>
	</validator>
</validators>